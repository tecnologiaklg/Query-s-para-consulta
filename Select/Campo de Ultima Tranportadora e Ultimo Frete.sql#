#type.sql# 
SELECT TGFPAR.RAZAOSOCIAL
FROM TGFCAB,
TGFPAR
WHERE
TGFCAB.CODPARCTRANSP = TGFPAR.CODPARC
AND TGFCAB.CODTIPOPER IN (3200, 3205, 3250, 3262) 
AND TGFCAB.CODPARC = TGFPAR.CODPARC
AND TGFCAB.DTNEG = (SELECT MAX(TGFCAB.DTNEG) FROM TGFCAB JOIN TGFPAR ON TGFCAB.CODPARC = TGFPAR.CODPARC
WHERE TGFCAB.CODTIPOPER IN (3200, 3205, 3250, 3262)  AND TGFCAB.CODPARC=TGFPAR.CODPARC)


#type.sql# 
SELECT DECODE(TGFCAB.CIF_FOB,'C','CIF','F','FOB','S','SEM FRETE','T','TERCEIRO')
FROM TGFCAB,
TGFPAR
WHERE
TGFCAB.CODPARCTRANSP = TGFPAR.CODPARC
AND TGFCAB.CODTIPOPER IN (3200, 3205, 3250, 3262) 
AND TGFCAB.CODPARC = TGFPAR.CODPARC
AND TGFCAB.DTNEG = (SELECT MAX(TGFCAB.DTNEG) FROM TGFCAB JOIN TGFPAR ON TGFCAB.CODPARC = TGFPAR.CODPARC
WHERE TGFCAB.CODTIPOPER IN (3200, 3205, 3250, 3262)  AND TGFCAB.CODPARC=TGFPAR.CODPARC)